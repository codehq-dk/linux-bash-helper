{
    "name": "codehq-dk/linux-bash-helper",
    "description": "Small helper to make life easier when running bash commands in php with exec command ",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1"
    },
    "autoload": {
        "psr-4": {
            "CodeHqDk\\LinuxBashHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeHqDk\\LinuxBashHelper\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests",
        "test-report": "vendor/bin/phpunit tests/ --coverage-html tests/CoverageReport"
    }
}
